<!DOCTYPE html>
<html>
  <head>
    <title>JustbitSocial</title>
    <%= csrf_meta_tags %>
    <%=
    meta_tags(
      :title => "Justbit tickets",
      :description => "Para todos nuestros clientes",
      :og => {:type => "website"},
      :ns => {:my_custom_meta => "tickets, clients"}
    )
    %>
    <%= stylesheet_link_tag    'application', 'https://fonts.googleapis.com/css?family=Lato:300,400,700,900', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'registration' %>
  </head>

  <body>
    <p><%= notice %><%= alert %> </p>
    <%= yield %>
  </body>
</html>
